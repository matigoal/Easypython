for num in range(1, 101): #initialisation de la boucle dans un tableau
    if num % 3 == 0 and num % 5 == 0: #premier cas
        print('FizzBuzz')
    elif num % 3 == 0: #deuxième cas
        print('Fizz')
    elif num % 5 == 0:
        print('Buzz')#troisième cas
    else:
        print(num) #résultat
